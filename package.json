{
  "name": "@undrstnd/ai-engine",
  "version": "2.1.0",
  "description": "Integrate advanced AI capabilities into your applications with our comprehensive SDK.",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "tsc",
    "dev": "vitest",
    "test": "vitest run",
    "ci": "pnpm run lint && pnpm run test && pnpm run build",
    "release": "pnpm run ci && changeset publish",
    "fix": "prettier . --write",
    "generate": "prisma generate && prisma db push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/undrstnd-labs/ai-engine.git"
  },
  "keywords": [
    "sdk",
    "engine",
    "ai-tools",
    "dev-tools",
    "openai",
    "vercel-ai-sdk",
    "stream-ui"
  ],
  "author": "undrstnd-labs",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/undrstnd-labs/ai-engine/issues"
  },
  "homepage": "https://github.com/undrstnd-labs/ai-engine#readme",
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "@types/node": "^22.5.4",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5",
    "prisma": "^5.19.1"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.58",
    "@ai-sdk/provider": "^0.0.23",
    "@ai-sdk/provider-utils": "^1.0.18",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@prisma/client": "^5.19.1",
    "ai": "^3.3.28",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "zod": "^3.23.8"
  }
}
